<?xml version="1.0"?>
<robot name="office">

   <material name="Brown">
     <color rgba="0.33 0.21 0.04 1"/>
   </material>

   <material name="White">
    <color rgba="1 1 1 1"/>
   </material>

   <material name="Grey">
    <color rgba=".3 .3 .3 1"/>
   </material>

    <material name="Red">
     <color rgba="1 0 0 1"/>
    </material>

    <material name="Green">
     <color rgba="0 1 0 1"/>
    </material>

    <material name="Blue">
     <color rgba="0 0 1 1"/>
    </material>

   <link name="ground">
   	<inertial>
   		<mass value=".5" />
   		<origin xyz="0 0 0" rpy="0 0 0"/>
		<inertia ixx="10"  ixy="0"  ixz="0" iyy="10" iyz="0" izz="10" />
   	</inertial>
  	<visual>
      <origin xyz="0 5 -0.25" rpy="0 0 0" />
      <geometry>
        <box size="10 15 0.5" />
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <origin xyz="0 5 -0.5" rpy="0 0 0" />
      <geometry>
        <box size="10 15 0.5" />
      </geometry>
    </collision>
   </link>

   <link name="obs1">
   	<inertial>
   		<mass value=".5" />
   		<origin xyz="0 0 0" rpy="0 0 0"/>
		<inertia ixx="10"  ixy="0"  ixz="0" iyy="10" iyz="0" izz="10" />
   	</inertial>
  	<visual>
      <origin xyz="2 4 1" rpy="0 0 0" />
      <geometry>
        <box size="6 0.1 2" />
      </geometry>
      <material name="Green"/>
    </visual>
    <collision>
      <origin xyz="2 4 1" rpy="0 0 0" />
      <geometry>
        <box size="6 .1 2." />
      </geometry>
    </collision>
   </link>

   <link name="obs2">
   	<inertial>
   		<mass value=".5" />
   		<origin xyz="0 0 0" rpy="0 0 0"/>
		<inertia ixx="10"  ixy="0"  ixz="0" iyy="10" iyz="0" izz="10" />
   	</inertial>
  	<visual>
      <origin xyz="-2 7 1" rpy="0 0 0" />
      <geometry>
        <box size="1 1 2" />
      </geometry>
      <material name="Red"/>
    </visual>
    <collision>
      <origin xyz="-2 7 1" rpy="0 0 0" />
      <geometry>
        <box size="1 1 2" />
      </geometry>
      <material name="Red"/>
    </collision>
   </link>

   <link name="obs3">
   	<inertial>
   		<mass value=".5" />
   		<origin xyz="0 0 0" rpy="0 0 0"/>
		<inertia ixx="10"  ixy="0"  ixz="0" iyy="10" iyz="0" izz="10" />
   	</inertial>
  	<visual>
      <origin xyz="-3 2 1" rpy="0 0 0" />
      <geometry>
        <box size="1 1 2" />
      </geometry>
      <material name="Red"/>
    </visual>
    <collision>
      <origin xyz="-3 2 1" rpy="0 0 0" />
      <geometry>
        <box size="1 1 2" />
      </geometry>
      <material name="Red"/>
    </collision>
   </link>

		<joint name="obs2_to_ground" type="fixed">
			<parent link="ground"/>
			<child link="obs2"/>
			<origin xyz="0 0 0"/>
		</joint>

		<joint name="obs1_to_ground" type="fixed">
			<parent link="ground"/>
			<child link="obs1"/>
			<origin xyz="0 0 0"/>
		</joint>

		<joint name="obs3_to_ground" type="fixed">
			<parent link="ground"/>
			<child link="obs3"/>
			<origin xyz="0 0 0"/>
		</joint>

</robot>
